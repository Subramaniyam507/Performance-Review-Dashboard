{
  "name": "Performance-Review-Dashboard",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js-community/odata-v2-adapter": "^1.15.9",
    "@cap-js/hana": "^2",
    "@sap/cds": "^9",
    "express": "^4",
    "i": "^0.3.7",
    "npm": "^11.10.0"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9",
    "cds-plugin-ui5": "^0.13.0",
    "rimraf": "^5.0.5",
    "mbt": "^1.2.29"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-smartcontroltut": "cds watch --open com.performance.smartcontroltut/index.html?sap-ui-xx-viewCache=false --livereload false",
    "undeploy": "cf undeploy Performance-Review-Dashboard --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "private": true,
  "cds": {
    "requires": {
      "destinations": true
    }
  },
  "workspaces": [
    "app/*"
  ]
}
